keywords = {Analytical models,Current Flow Controller,DC/DC converter,Distributed Current Flow Control,HVDC transmission,Integrated circuit modeling,Mathematical model,Meshed HVDC grid,Topology,Voltage control},
pages = {Early Access},
title = {{Selective Operation of Distributed Current Flow Controller Devices for Meshed HVDC Grids}},
